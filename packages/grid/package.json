{
  "name": "@coleski/datagrid",
  "version": "0.8.1",
  "description": "Ultra-fast canvas-based data grid for React",
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist *.tsbuildinfo",
    "preview": "previewjs"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.17",
    "@types/react-dom": "^18.3.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tsup": "^8.3.5",
    "typescript": "^5.7.3",
    "@previewjs/cli": "^1.30.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/coleski/datagrid.git",
    "directory": "packages/grid"
  },
  "homepage": "https://github.com/coleski/datagrid#readme",
  "bugs": {
    "url": "https://github.com/coleski/datagrid/issues"
  },
  "keywords": [
    "react",
    "grid",
    "data-grid",
    "table",
    "canvas",
    "virtual",
    "performance"
  ],
  "author": "Archer Hume <https://github.com/archerhume>",
  "maintainers": [
    "Cole Benefield <https://github.com/coleski>"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
